{
  "name": "halterra",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "sideEffects": [
    "*.css"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
