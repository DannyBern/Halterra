    "vercel": "vercel --prod --yes",
    "optimize-images": "node scripts/optimize-images.mjs"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.68.0",
    "@clerk/clerk-react": "^5.57.1",
    "@clerk/clerk-sdk-node": "^4.13.23",
